extends layout

block content
  span(class="centeringForm")
    p
      h1 Entered
    p
      h5 First Name:
        p #{entries[0]}
      h5 Last Name:
        p #{entries[1]}
      h5 Age:
        p #{entries[2]}
      h5 Gender:
        p #{entries[3]}
      h5 Longitude Major Ticks:
        p [ #{entries[4]} ]
      h5 Longitude Minor Ticks:
        p [ #{entries[5]} ]
      h5 Number Of Nodes:
        p #{entries[6]}
      h5 Clip Latitude At:
        p #{entries[7]}
      h5 Latitude Tick Interval:
        p #{entries[8]}
      h5 Mapscale:
        p  1/#{entries[9]}
      h5 Height:
        p #{entries[10]} cm

      p
        a(href='/form') 
          button(class="mdl-button mdl-js-button mdl-button--fab mdl-button--colored mdl-js-ripple-effect", type='submit', title='Submit your requests')
            i(class="material-icons") arrow_back 
